{
  "parser": "babel-eslint",
  "plugins": [
    "import"
  ],
  "rules": {
    "import/newline-after-import": 0,
    "import/extensions": 0,
    "no-unused-vars": 0,
    "no-console": 0,
    "no-restricted-syntax": 0,
    "no-prototype-builtins": 0,
    "no-param-reassign": 0,
    "no-useless-escape": 0,
    "no-cond-assign": 0,
    "no-plusplus": 0,
    "no-underscore-dangle": 0,
    "no-continue": 0,
    "comma-dangle": 0,
    "eol-last": 0,
    "no-eval": 0,
    "curly": 0,
    "nonblock-statement-body-position": 0,
    "max-len": 0,
    "linebreak-style": 0,
    "indent": 0,
    "func-names": 0,
    "eqeqeq": 0,
    "object-curly-newline": 0,
    "lines-between-class-members": 0
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}